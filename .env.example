# Raindrop.io OAuth (required)
# Get your credentials at: https://app.raindrop.io/settings/integrations
RAINDROP_CLIENT_ID=your_client_id
RAINDROP_CLIENT_SECRET=your_client_secret
RAINDROP_REDIRECT_URI=http://localhost:3000/callback

# X.com OAuth (optional - for bookmark import feature)
# Get your credentials at: https://developer.twitter.com/en/portal/dashboard
X_CLIENT_ID=your_x_client_id
X_CLIENT_SECRET=your_x_client_secret
X_REDIRECT_URI=http://localhost:3001/callback
